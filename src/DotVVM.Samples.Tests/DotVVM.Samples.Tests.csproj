<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />
  <PropertyGroup>
    <OutputType>library</OutputType>
    <TargetFramework>net461</TargetFramework>
    <RuntimeIdentifiers>win7-x86;win7-x64</RuntimeIdentifiers>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="App.config" />
    <None Include="App.ANC_Chrome.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.ANC_Firefox.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.ANC_InternetExplorer.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.Owin_Chrome.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.Owin_Firefox.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.Owin_InternetExplorer.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="App.Release.config">
      <DependentUpon>App.config</DependentUpon>
    </None>
    <None Include="SamplesRouteUrls.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>SamplesRouteUrls.cs</LastGenOutput>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Sdk">
      <Version>1.0.0-alpha-20161104-2</Version>
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.NET.Test.Sdk">
      <Version>15.0.0-preview-20161123-03</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Owin.Security">
      <Version>3.0.1</Version>
    </PackageReference>
    <PackageReference Include="Moq">
      <Version>4.6.38-alpha</Version>
    </PackageReference>
    <PackageReference Include="Riganti.Utils.Testing.Selenium.Core">
      <Version>1.0.0-beta</Version>
    </PackageReference>
    <PackageReference Include="Riganti.Utils.Testing.Selenium.DotVVM">
      <Version>0.1.0-pre</Version>
    </PackageReference>
    <PackageReference Include="Selenium.Support">
      <Version>2.53.1</Version>
    </PackageReference>
    <PackageReference Include="Selenium.WebDriver">
      <Version>2.53.1</Version>
    </PackageReference>
    <PackageReference Include="SlowCheetah">
      <Version>2.5.15</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
  </ItemGroup>
  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="SamplesRouteUrls.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SamplesRouteUrls.tt</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DotVVM.Core\DotVVM.Core.csproj" />
    <ProjectReference Include="..\DotVVM.Framework\DotVVM.Framework.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.VisualStudio.QualityTools.UnitTestFramework">
      <HintPath>C:\Program Files (x86)\Microsoft Visual Studio\2017\Enterprise\Common7\IDE\ReferenceAssemblies\v4.0\Microsoft.VisualStudio.QualityTools.UnitTestFramework.dll</HintPath>
    </Reference>
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <UsingTask TaskName="TransformXml" AssemblyFile="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v15.0\Web\Microsoft.Web.Publishing.Tasks.dll" />
  <Target Name="AfterCompile" Condition="exists('App.$(Configuration).config')">
    <Message Importance="high" Text="Transforming config" />
    <TransformXml Source="App.config" Destination="$(IntermediateOutputPath)$(TargetFileName).config" Transform="App.$(Configuration).config" />
    <Message Importance="high" Text="Config transformed to $(IntermediateOutputPath)$(TargetFileName).config" />
    <ItemGroup>
      <AppConfigWithTargetPath Remove="App.config" />
      <AppConfigWithTargetPath Include="$(IntermediateOutputPath)$(TargetFileName).config">
        <TargetPath>$(TargetFileName).config</TargetPath>
      </AppConfigWithTargetPath>
    </ItemGroup>
  </Target>
</Project>