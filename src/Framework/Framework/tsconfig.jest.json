{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitThis": true,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2020",
    "declaration": true,
    "strictNullChecks": true,
    "lib": [ "dom", "ES2020" ],
    "module": "ES2015",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [ "obj/**", "node_modules/**" ],
  "compileOnSave": false
}
