{
    "compilerOptions": {
      "noImplicitAny": true,
      "strictFunctionTypes": true,
      "strict": true,
      "noImplicitThis": true,
      "noEmitOnError": false,
      "removeComments": false,
      "sourceMap": true,
      "target": "ES2018",
      "declaration": true,
      "strictNullChecks": true,
      "lib": [ "dom", "es2015.promise", "es5", "es6" ],
      "module": "ES2015",
      "esModuleInterop": true
    },
    "exclude": ["obj/**", "node_modules/**"]
}